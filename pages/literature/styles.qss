/* QTextBrowser styling */
QTextBrowser {
    font-size: 16px;
}

/* QLabel Style */
QLabel#litTitle {
    font-size: 18px;
    font-weight: bold;
    margin-bottom: 5px;
}

/* QFrame Style */
QFrame#litBox {
    background-color: #F9FBFF;  /* very subtle blue tint */
    border: 1px solid #CCC;
    border-radius: 5px;
}
